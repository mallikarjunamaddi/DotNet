@model IEnumerable<EmployeeManagement.Models.Employee>
	<html xmlns="http://www.w3.org/1999/xhtml">
	<head>
		<title></title>
	</head>
	<body>
		<table>
			<tr>
				<td>ID</td>
				<td>Name</td>
				<td>Email</td>
			</tr>
			@foreach (var employee in Model)
			{
			   <tr>
				   <td>@employee.Id</td>
				   <td>@employee.Name</td>
				   <td>@employee.Email</td>
			   </tr>
			}
		</table>
	</body>
	</html>