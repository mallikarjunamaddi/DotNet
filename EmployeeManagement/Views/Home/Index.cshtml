@model IEnumerable<EmployeeManagement.Models.Employee>

@{
	Layout = "~/Views/Shared/_Layout.cshtml";
	ViewBag.Title = "Index";
}

<table>
	<tr>
		<td>ID</td>
		<td>Name</td>
		<td>Email</td>
	</tr>
	@foreach (var employee in Model)
	{
		<tr>
			<td>@employee.Id</td>
			<td>@employee.Name</td>
			<td>@employee.Email</td>
		</tr>
	}
</table>

@section Scripts {
	<script src="~/js/CustomScript.js"></script>
}